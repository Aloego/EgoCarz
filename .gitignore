{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn"],
    "no-undef": "error",
    "no-console": "off",
    "eqeqeq": "warn",
    "curly": "warn",
    "semi": ["warn", "always"],
    "quotes": ["warn", "single"],
    "no-debugger": "warn"
  }
}

